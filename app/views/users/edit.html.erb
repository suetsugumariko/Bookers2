
<%#= form_with model: @user do |f| %>
<%= form_with model: @user,url:user_path(@user.id) do |f| %>
<%#= form_with model: @user,url:"/users/#{@user.id}" do |f| %>

  <h2>User info</h2>

  <h4>Name</h4>
  <%= f.text_field :name %>

  <h4>Image</h4>
  <%= f.file_field :image, accept: "image/*" %>

  <h4>Introduction</h4>
  <%= f.text_area :introduction %>


  <%= f.submit 'Update User' %>

<% end %>

